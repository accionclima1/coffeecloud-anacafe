                        <td ng-click="loadTest(row);">{{row.municipio}}</td>
                        <td ng-click="loadTest(row);">{{row.ubicacion}}</td> 
                        <td ng-click="loadTest(row);">{{row.recomendaciontecnica}}</td> 
