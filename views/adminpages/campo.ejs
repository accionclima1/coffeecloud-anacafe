						     <div class="form-group ">
						      <label class="control-label " for="otro">
						       Otro
						      </label>
						      <input class="form-control" id="otro" name="otro" type="text" ng-model="campodata.otro" required="true" />
						     </div>
						     <div class="form-group ">
						      <label class="control-label " for="nudos">
