<script type="text/ng-template" id="/supportExtInterna.html">

	<!-- Modal -->
			<div class="modal fade" id="myModalUserImage" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
				<div class="modal-dialog" role="document">
					<div class="modal-content">

						<div class="modal-header">
							<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
							<h4 class="modal-title" id="myModalLabel">Imagen de Usuario</h4>
						</div>
						<div class="modal-body">
							<div ng-show="error" class="alert alert-danger row">
								<span>{{ error.message }}</span>
							</div>
							<div class="form-group">
								<label>Image:</label>
								<input type="file" id="userImage" file-model="myFile" />
							</div>
							<div class="form-group">
								<div ng-show="message" class="bg-success ">
									<span>{{ message }}</span>
								</div>
								<div ng-show="error" class="alert alert-danger ">
									<span>{{ error.message }}</span>
								</div>
							</div>
						</div>
						<div class="modal-footer">
							<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
							<button type="button" ng-click="uploadPhoto()" class="btn btn-primary">Save</button>
						</div>

					</div>
				</div>
			</div>
			<!-- Modal -->
		<div class="support-wrap" ng-controller="SupportExtInternaCtrl">
			<h2 class="section-header userName">
				<a class="back-homeint" style="position: fixed;left: 20px;color: #FFF;" href="#/supportext">
				 <i style="font-size: 20px;" class="fa fa-chevron-left"></i>
				</a>
				<img class="userImage chat-image chat-image-header" ng-src="images/ChatUser.png" src="images/ChatUser.png">
				{{senderUser}}</h2>
			<div class="container">

				<div class="row chatUser">
					<div class="col-md-6 col-md-offset-3">
						<div class="chatbox" scroll-glue>
							<div class="message" ng-repeat="message in chatLog" ng-style="{'float':(senderUser == message.sender ? 'left' : 'right')}">
								<div class="message-wrap">
									<!-- <img class="userImage chat-image" ng-src="{{ UserImage }}" />&nbsp;&nbsp;<br> -->
									<div class="message-body chat-Body">
										<p>{{ message.bodyMsg }}</p>
									</div>
									<div ng-if="message.Msgattachement" class="album">
										<img class="attachment-image" ng-src="{{ message.Msgattachement }}" />
									</div>
								</div>

							</div>
							<div style="clear:both"></div>
						</div>

					</div>
				</div>

			</div>
		</div>
		<form class="type-sink form-inline sendMesseges"  ng-submit="sendMessage(files)" ng-controller="SupportExtInternaCtrl">
			<div class="form-group relativepos">
				<a ng-click="openImagePopup()"><img style="margin-top:10px" class="userImage chat-image" ng-src="{{ UserImageBottom }}" /></a>
				<!--<label class="chat-text-bottom">{{ UserName }}</label>-->
				<label class="sr-only" for="chatMsg">Chat message</label>
				<!-- <div class="watschat"> -->
			    	<input type="text" class="form-control" name="chatMsg" id="chatMsg">
			    	<!--<i class="fa fa-paperclip fa-5 icattache" onclick="$('#filuploadattach').click();$('#filuploadattach').trigger('input')"></i>-->
				    <!-- <input type="file" class="" ng-model="myFile" name="chatattchment" id="filuploadattach"> -->
				    <i class="fa fa-paperclip fa-5 icattache" ngf-select ng-model="files" ngfDataUrl></i>

			    <!-- </div> -->
				<input type="hidden" name="fromId" value="{{ currentUser() }}">
		    <input class="chatUser" type="hidden" name="toId" value="chatUserName">
		  </div>
		  <button type="submit" class="btn btn-default"><i class="fa fa-location-arrow"></i></button>
		</form>
	</script>
